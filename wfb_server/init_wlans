import psutil
import click
import rich

@click.command()
@click.option('--wfb_cfg', default='wfb_server.cfg', help='target cfg file')
@click.option('--cfg', default='wlan_init', help='cfg file')

def main(wfb_cfg,cfg):
    #print(psutil.net_connections('udp'))
    rich.print(psutil.net_if_addrs())
    
if __name__ == '__main__':
    main()